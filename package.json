{
  "name": "webapp",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "wrangler pages dev",
    "deploy": "npm run build && wrangler pages deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "db:migrate:local": "wrangler d1 migrations apply ramat-production --local",
    "db:migrate:prod": "wrangler d1 migrations apply ramat-production",
    "db:console:local": "wrangler d1 execute ramat-production --local",
    "db:console:prod": "wrangler d1 execute ramat-production"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "hono": "^4.10.8"
  },
  "devDependencies": {
    "@hono/vite-build": "^1.2.0",
    "@hono/vite-dev-server": "^0.18.2",
    "vite": "^6.3.5",
    "wrangler": "^4.4.0"
  }
}
